doctype html
head

  
  meta(property='og:site_name' content='MusicSOL')
  meta(property='og:type' content='website')
  meta(charset='utf-8')
  
  block continue_head

  link(rel='icon' href='assets/img/favicon.png')
  // Font Awesome icons (free version)
