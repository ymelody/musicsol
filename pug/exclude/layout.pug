// Navigation
span#page-top
nav#mainNav.navbar.navbar-expand-lg.navbar-light.fixed-top
  .container
    // <a class="navbar-brand js-scroll-trigger logo-font" href="/#page-top">MusicSOL</a>
    a.navbar-brand.js-scroll-trigger(href='/#page-top')
      img(src='./assets/img/logo.svg' alt='MusicSOL logo')
    button#menu-btn.navbar-toggler.navbar-toggler-right(type='button' data-toggle='collapse' data-target='#navbarResponsive' aria-controls='navbarResponsive' aria-expanded='false' aria-label='Toggle navigation')
      | Menu
      i.fas.fa-bars(style='margin-left:5px;')
    #navbarResponsive.collapse.navbar-collapse
      ul.navbar-nav.ml-auto
        li.nav-item
          a.nav-link.js-scroll-trigger(href='/#about') About
        li.nav-item
          a.nav-link.js-scroll-trigger(href='/#howitworks') How it works?
        // <li class="nav-item"><a class="nav-link js-scroll-trigger" href="#resources">Resources</a></li>
        li.nav-item
          a.nav-link.js-scroll-trigger(target='_new' href='https://musicsol.teachworks.com/form/student-signup') Sign up
        
        li.nav-item
          a.nav-link.js-scroll-trigger(href='/#pricing') Pricing

        li.nav-item
          a.nav-link(href="./login") Login
        
        li.nav-item
          a.nav-link.js-scroll-trigger(href='/#contact') Contact
// data-ride="carousel"
header#carousel.carousel.slide.masthead-nobg(data-ride='carousel')
  block below-header-container
block content

block footer
  footer.bg-black.text-white.text-center.text-lg-start(style='border-top: 1px solid var(--primary);')
    // Grid container
    .container.p-4
      // Grid row
      .row
        // Grid column
        .col-lg-6.col-md-12.mb-4.mb-md-0
          h5.text-uppercase Our Mission
          p
            | The mission of MusicSOL is to provide aspiring young musicians from across the country with an opportunity to study with world-class artists and to provide our students with a music education of the highest artistic standards.
        // Grid column
        // Grid column
        .col-lg-3.col-md-6.mb-4.mb-md-0
          ul.list-unstyled.mb-0
            li
              a.nav-link.js-scroll-trigger(href='/#page-top') Home
            li
              a.nav-link.js-scroll-trigger(href='/#about') About
            li
              a.nav-link.js-scroll-trigger(href='/#howitworks') How it works
            li
              a.nav-link.js-scroll-trigger(href='https://musicsol.teachworks.com/form/student-signup' target='_new') Student sign up

        // Grid column
        // Grid column
        .col-lg-3.col-md-6.mb-4.mb-md-0
          ul.list-unstyled
            li
              a.nav-link.js-scroll-trigger(href='https://blog.musicsol.net') Blog
            //- li
              a.nav-link.js-scroll-trigger(href='#') Work with us
            li
              a.nav-link.js-scroll-trigger(href='/terms-and-conditions') Terms and Conditions
            li
              a.nav-link(href='/teachers') Our Teachers
        // Grid column
      // Grid row
    // Grid container
    // Copyright
    .bg-white.text-black.text-center.p-3
      | &copy; &nbsp;
      span#copyrightyear
      |&nbsp;Copyright MusicSOL LLC.
      script.
        document.getElementById('copyrightyear').innerText = new Date().getFullYear()
      
    // Copyright
